{
  "name": "utilyscript",
  "version": "0.0.0-development",
  "description": "A micro utility library for the modern frontend developer",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Riccardo-Zanutta/utilyscript.git"
  },
  "keywords": [
    "utilities",
    "javascript"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Riccardo Zanutta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Riccardo-Zanutta/utilyscript/issues"
  },
  "homepage": "https://github.com/Riccardo-Zanutta/utilyscript#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
